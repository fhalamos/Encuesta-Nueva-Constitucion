<!DOCTYPE html>
<html>




<head>
  <title>Constitucion</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>

<body ng-app='encuestaNuevaConstitucion'>

  <%= render 'layouts/header' %>
  <div class="row page-title">
    <div class = "container">
      <div class="col s12">
        <div class="subtitle">
          <%= link_to 'Portada ' %>/
        </div>
        <h3>Encuesta</h3>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="container">
      <div class="row">
        <div class="col s12">


        </div>
      </div>


      <div class="row">

        <div class="col s12">
          <%= yield %>
        </div>
      </div>

    </div>

  </div>

  <%= render 'layouts/footer' %>

</body>



</html>
