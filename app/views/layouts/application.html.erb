<!DOCTYPE html>
<html>




<head>
  <title>Constitucion</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>

<body ng-app='encuestaNuevaConstitucion'>

  <div class="row">
    <div class="row">
      <div class="col s12">
        <p class="notice"><%= notice %></p>
        <p class="alert"><%= alert %></p>

        <p>
          <% if user_signed_in? %>
          <%= link_to 'Desconectar', destroy_user_session_path, :method => :delete, class: 'btn red' %>
          <% end %>
        </p>
      </div>
    </div>

    <div class="row">
      <div class="col s12">
        <%= yield %>
      </div>
    </div>

  </div>

</body>



</html>
